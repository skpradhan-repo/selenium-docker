<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name="book-flight-module" parallel="tests">
    <test name="book-flight-2">
        <parameter name="noOfPassengers" value="2" />
        <parameter name="expectedPrice" value="$1169 USD" />
        <classes>
            <class name="com.samsoft.newtours.test.BookFlightTest" />
        </classes>
    </test>
    <test name="book-flight-3">
        <parameter name="noOfPassengers" value="3"></parameter>
        <parameter name="expectedPrice" value="$1753 USD"></parameter>
        <classes>
            <class name="com.samsoft.newtours.test.BookFlightTest"></class>
        </classes>
        $1753
    </test>
</suite>